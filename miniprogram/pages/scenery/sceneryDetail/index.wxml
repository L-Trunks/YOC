<view>
	<view class='info_box'>
		<view>
			<swiper style='height:364rpx'>
				<block wx:for="{{sceneryInfo.picList}}" wx:key="index">
					<swiper-item>
						<image
						 style='width:100%;height:364rpx'
						 src="{{item.picUrl}}"
						 mode="aspectFit"
						 lazy-load="false"
						 binderror=""
						 bindload=""
						/>
					</swiper-item>
				</block>
			</swiper>
		</view>
		<view>
			<view class='flex' style='margin:40rpx 0;'>
				<view style='font-size:48rpx'>
					{{sceneryInfo.name || ''}}
				</view>
				<view bindtap="goLocation" class='flex j_end' style='font-size:20rpx;font-weight:500;color:#000'>
					<image src="../../../images/scenery/daohang_03.png" style='width:84rpx;height:78rpx;' />
					<view style="white-space: nowrap;">
						转到导航
					</view>
				</view>
			</view>
			<view>
				<view class='position_box'>
					<block wx:for="{{listItem}}" wx:key="index">
						<view class='cellList' bindtap='goPage'>
							<view class='left'>
								<image src='{{item.src}}' mode='widthFix' />
							</view>
							<view class='middle'>
								<text>{{item.text}}</text>
							</view>
							<view bindtap="showDetail" data-detail="{{item.label}}" class='label'>{{item.label || ""}}</view>
							<view class='right'>
								<image src='../../../images/scenery/more_icon_23.png' />
							</view>
						</view>
					</block>
				</view>
			</view>
		</view>
	</view>
	<image
	 class="background_img"
	 src="../../../images/scenery/background_14.png"
	 mode="aspectFit|aspectFill|widthFix"
	 lazy-load="false"
	 binderror=""
	 bindload=""
	/>
</view>

