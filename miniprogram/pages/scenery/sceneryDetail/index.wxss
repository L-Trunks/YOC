@import '../../../style/base.wxss';
.info_box{
    padding: 100rpx 70rpx 0 70rpx;
}
.cellList {
    height: 100rpx;
    width: 100%;
    display: flex;
    border-bottom: 1rpx solid #f5f5f5;
}

.cellList>view {
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.cellList>.left {
    padding-left: 10rpx;
    width: 56rpx;
}

.left>image {
    width: 50rpx;
    height: 60rpx;
}

.cellList>.middle {
    position: relative;
    padding-left: 10rpx;
    width: 160rpx;
    color: #000;
    font-size: 26rpx;
    font-weight: 600;
}

.cellList>.right {
    width: 50rpx;
    align-items: flex-end;
}

.right>image {
    width: 45rpx;
    height: 55rpx;
}

.label {
    flex: 1;
    color: rgb(44, 44, 44);
    font-size: 24rpx;
    margin-right: 50rpx;
    padding-left: 50rpx;
    display: inline-block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.position_box {
    position: relative;
}

.background_img{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 380rpx;
    height: 380rpx;
}